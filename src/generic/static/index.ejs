<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>DJEN - Metal Generator</title>

	<% if (htmlWebpackPlugin.options.isProduction) { %>
	<script>
		if ('serviceWorker' in navigator) {
			navigator.serviceWorker.register('/sw.js')
				.then(function (d) { console.log('sw.js registered') })
				.catch(function (e) { console.log(e, 'sw.js NOT registered') });
		}
	</script>
	<% } %>

	<link href="https://fonts.googleapis.com/css?family=Open+Sans:400,300,700,600" rel="stylesheet" type="text/css"media="screen" async>
	<link rel="manifest" href="<%= htmlWebpackPlugin.options.absolutePath %>manifest.json">

	<meta name="theme-color" content="#1e1e1e">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1">
	<meta name="description" content="Rock and Metal Song Generator.">
	<meta name="keywords" content="djeneration, station, generative, react, redux, metal, riff, generator, thall, djent, generate">

	<!-- Schema.org markup for Google+ -->
	<meta itemprop="name" content="DJEN">
	<meta itemprop="description" content="Rock and Metal Song Generator.">
	<meta itemprop="image" content="http://djen.co/assets/images/icon.png">

	<!-- Twitter Card data -->
	<meta name="twitter:card" content="summary">
	<meta name="twitter:title" content="DJEN">
	<meta name="twitter:description" content="Rock and Metal Song Generator.">
	<meta name="twitter:creator" content="@RossMcMillan92">
	<meta name="twitter:image" content="http://djen.co/assets/images/icon.png">

	<!-- Open Graph data -->
	<meta property="og:title" content="DJEN">
	<meta property="og:type" content="website">
	<meta property="og:description" content="Rock and Metal Song Generator.">
	<meta property="og:url" content="http://djen.co/">
	<meta property="og:image" content="http://djen.co/assets/images/icon.png">

	<!-- Apple data -->
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="apple-mobile-web-app-status-bar-style" content="#1e1e1e">
	<meta name="apple-mobile-web-app-title" content="DJEN">
	<meta name="format-detection" content="telephone=no">

	<link rel="apple-touch-icon" sizes="57x57" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/57.png">
	<link rel="apple-touch-icon" sizes="60x60" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/60.png">
	<link rel="apple-touch-icon" sizes="72x72" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/72.png">
	<link rel="apple-touch-icon" sizes="76x76" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/76.png">
	<link rel="apple-touch-icon" sizes="114x114" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/114.png">
	<link rel="apple-touch-icon" sizes="120x120" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/120.png">
	<link rel="apple-touch-icon" sizes="144x144" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/144.png">
	<link rel="apple-touch-icon" sizes="152x152" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/152.png">
	<link rel="apple-touch-icon" sizes="180x180" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/180.png">
	<link rel="icon" type="image/png" sizes="192x192"  href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/192.png">
	<link rel="icon" type="image/png" sizes="96x96" href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/96.png">
	<meta name="msapplication-TileColor" content="#1e1e1e">
	<meta name="msapplication-TileImage" content="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/144.png">

	<meta http-equiv="Content-Security-Policy"
	         content="default-src * blob:;
	                  style-src * 'self' 'unsafe-inline' 'unsafe-eval';
	                  script-src * 'self' 'unsafe-inline' 'unsafe-eval';">
	<noscript>
		<style> .js { display: none !important } </style>
	</noscript>
</head>
<body class="preload">
	<div id="root"></div>
	<div class="splash-screen u-flex-row u-flex-center u-flex-justify-center" splash-screen>
		<div class="splash-screen__body">
			<img class="splash-screen__logo u-mb1" src="/assets/images/logo.png" alt="djen.co djen logo">
			<noscript>
				<div class="splash-screen__text">JavaScript is required for djen.co to run</div>
			</noscript>
			<div class="js u-flex-row u-flex-justify-center">
				<div class="spinner"></div>
			</div>
		</div>
	</div>

	<script>
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
		ga('create', 'UA-39207239-2', 'auto');
		ga('send', 'pageview');
	</script>
</body>
</html>
